{
    "env": {
        "browser": true,
        "amd": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ]
    }
}